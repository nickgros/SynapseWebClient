<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
>
  <ui:with
    field='icons'
    type='org.sagebionetworks.web.client.IconsImageBundle'
  />
  <g:FlowPanel addStyleNames="flexcontainer-row margin-top-5">
    <g:FlowPanel addStyleNames="flexcontainer-column">
      <b:CheckBox ui:field="select" addStyleNames="inline-block" />
    </g:FlowPanel>

    <g:FlowPanel
      ui:field="userBadgeContainer"
      addStyleNames="flexcontainer-column flexcontainer-column-fill-width"
    />

    <g:FlowPanel
      ui:field="renewRevokeContainer"
      addStyleNames="flexcontainer-column flexcontainer-align-items-flex-end"
    >
      <b:ButtonGroup ui:field="renewRevokeButtonGroup" />
    </g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>
