<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
>
  <g:FlowPanel addStyleNames="filesBrowser margin-top-15">
    <g:FlowPanel
      ui:field="commandsContainer"
      addStyleNames="flexcontainer-row margin-bottom-10"
    >
      <g:FlowPanel
        addStyleNames="flexcontainer-column flexcontainer-column-fill-width"
      >
        <b:Heading size="H3" ui:field="title" text="Files" visible="false" />
      </g:FlowPanel>
      <g:FlowPanel
        addStyleNames="flexcontainer-column flexcontainer-align-items-flex-end"
        ui:field="actionMenuContainer"
      />
    </g:FlowPanel>
    <g:FlowPanel ui:field="addToDownloadListContainer" />
    <g:FlowPanel
      ui:field="files"
      styleName="highlight-box padding-top-0-imp overflow-auto margin-top-0"
    />
  </g:FlowPanel>
</ui:UiBinder>
