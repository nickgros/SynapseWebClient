<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <g:FlowPanel>
    <g:FlowPanel addStyleNames="flexcontainer-row">
      <g:FlowPanel
        addStyleNames="flexcontainer-column flexcontainer-column-fill-width font-size-30"
      >
        <g:FlowPanel
          addStyleNames="flexcontainer-row flexcontainer-align-items-center overflow-x-auto"
        >
          <w:EntityTypeIcon
            ui:field="entityIcon"
            addStyleNames="entityIcon margin-right-10 lightGreyText flexcontainer-column"
            type="folder"
          />
          <b:Heading
            addStyleNames="pageHeaderTitle flexcontainer-column"
            ui:field="fileName"
            size="H3"
          />
          <g:SimplePanel
            ui:field="favoritePanel"
            addStyleNames="flexcontainer-column margin-left-5"
          />
        </g:FlowPanel>
      </g:FlowPanel>
      <g:FlowPanel
        addStyleNames="flexcontainer-column flexcontainer-align-items-flex-end"
        ui:field="actionMenuContainer"
      />
    </g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>
