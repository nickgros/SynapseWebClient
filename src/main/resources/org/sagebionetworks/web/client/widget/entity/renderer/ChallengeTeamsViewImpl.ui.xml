<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <g:FlowPanel>
    <g:FlowPanel ui:field="loadingUI">
      <w:LoadingSpinner size="16px" />
      <bh:Text>
        &nbsp; Loading...
      </bh:Text>
    </g:FlowPanel>

    <b:Alert type="WARNING" ui:field="errorUI">
      <bh:Strong text="Markdown Widget Warning: " />
      <bh:Text ui:field="errorText" text="" />
    </b:Alert>

    <b:Panel ui:field="challengeTeamsUI" addStyleNames="margin-bottom-0-imp">
      <b:PanelBody>
        <g:FlowPanel
          ui:field="challengeTeamsContainer"
          addStyleNames="secondary-links"
        />
      </b:PanelBody>
    </b:Panel>
    <g:FlowPanel ui:field="paginationWidgetContainer" />
    <g:FlowPanel ui:field="dialogWidgetContainer" />
  </g:FlowPanel>
</ui:UiBinder>
