<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:t="urn:import:org.sagebionetworks.web.client.view.bootstrap.table"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <g:FlowPanel>
    <g:FlowPanel ui:field="synAlertContainer" />
    <t:Table ui:field="table" width="900px">
      <t:TableRow>
        <t:TableHeader text="Name" />
        <t:TableHeader text="Institution" />
        <t:TableHeader text="Email" />
      </t:TableRow>
    </t:Table>
    <w:LoadingSpinner
      ui:field="loadingUI"
      size="31px"
      addStyleNames="center-block center"
    />
    <g:FlowPanel ui:field="paginationWidgetContainer" />
  </g:FlowPanel>
</ui:UiBinder>
