<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
>
  <g:FlowPanel>
    <g:FlowPanel>
      <b:Heading size="H4" text="Edit Order">
        <w:HelpWidget
          helpMarkdown="Select a wiki page and use the arrows to move it to the desired position."
          href="https://help.synapse.org/docs/Wikis.1975746682.html#CreatingandManagingWikis-ModifyingPageOrder"
          addStyleNames="margin-left-5"
          placement="BOTTOM"
        />
      </b:Heading>
    </g:FlowPanel>
    <g:FlowPanel addStyleNames="center center-block margin-top-30">
      <g:FlowPanel>
        <b:VerticalButtonGroup>
          <b:Button ui:field="upButton" icon="ARROW_UP" />
        </b:VerticalButtonGroup>
      </g:FlowPanel>
      <g:FlowPanel>
        <b:ButtonGroup>
          <b:Button ui:field="leftButton" icon="ARROW_LEFT" />
          <b:Button ui:field="rightButton" icon="ARROW_RIGHT" />
        </b:ButtonGroup>
      </g:FlowPanel>
      <g:FlowPanel>
        <b:VerticalButtonGroup>
          <b:Button ui:field="downButton" icon="ARROW_DOWN" />
        </b:VerticalButtonGroup>
      </g:FlowPanel>
    </g:FlowPanel>
    <w:LoadingSpinner ui:field="loadingUI" size="31px" visible="false" />
    <g:SimplePanel ui:field="synAlertContainer" />
    <g:SimplePanel ui:field="treePanel" />

    <g:FlowPanel addStyleNames="center center-block">
      <g:FlowPanel>
        <b:VerticalButtonGroup>
          <b:Button ui:field="upButton2" icon="ARROW_UP" />
        </b:VerticalButtonGroup>
      </g:FlowPanel>
      <g:FlowPanel>
        <b:ButtonGroup>
          <b:Button ui:field="leftButton2" icon="ARROW_LEFT" />
          <b:Button ui:field="rightButton2" icon="ARROW_RIGHT" />
        </b:ButtonGroup>
      </g:FlowPanel>
      <g:FlowPanel>
        <b:VerticalButtonGroup>
          <b:Button ui:field="downButton2" icon="ARROW_DOWN" />
        </b:VerticalButtonGroup>
      </g:FlowPanel>
    </g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>
