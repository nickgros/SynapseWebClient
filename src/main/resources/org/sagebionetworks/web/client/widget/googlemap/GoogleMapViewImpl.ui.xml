<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <ui:with
    field='sageImageBundle'
    type='org.sagebionetworks.web.client.SageImageBundle'
  />
  <g:FlowPanel>
    <g:FlowPanel ui:field="synAlertContainer" />
    <g:FlowPanel visible="false">
      <g:FlowPanel ui:field="markerPopupContent">
        <b:Heading size="H4" ui:field="locationTitle" />
        <g:FlowPanel ui:field="userBadges" />
      </g:FlowPanel>
    </g:FlowPanel>
    <g:FlowPanel ui:field="googleMapContainer" height="300px">
      <g:FlowPanel
        ui:field="googleMapCanvas"
        height="100%"
        width="100%"
        addStyleNames="mapping"
      />
    </g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>
