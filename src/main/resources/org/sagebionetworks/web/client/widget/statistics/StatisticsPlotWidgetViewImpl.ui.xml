<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
  xmlns:w="urn:import:org.sagebionetworks.web.client.widget"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
>
  <b:Modal
    ui:field="statsPlotModal"
    title=""
    addStyleNames="modal-90-percent"
    closable="true"
    dataBackdrop="STATIC"
    dataKeyboard="true"
  >
    <b:ModalBody>
      <w:ReactComponentDiv ui:field="srcContainer" />
      <g:FlowPanel>
        <bh:Paragraph addStyleNames="center">
          This data is collected and processed on a monthly basis.
        </bh:Paragraph>
      </g:FlowPanel>
    </b:ModalBody>
    <b:ModalFooter>
      <b:Button ui:field="closeButton" type="DEFAULT" text="Close" />
    </b:ModalFooter>
  </b:Modal>
</ui:UiBinder>
