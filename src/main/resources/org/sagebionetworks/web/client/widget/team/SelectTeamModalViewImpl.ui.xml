<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
  xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
>
  <g:FlowPanel>
    <b:Modal
      ui:field="teamModal"
      closable="true"
      dataBackdrop="STATIC"
      dataKeyboard="true"
    >
      <b:ModalHeader>
        <b:Heading
          ui:field="title"
          size="H4"
          text="Select Team"
          addStyleNames="displayInline"
        />
      </b:ModalHeader>
      <b:ModalBody>
        <g:FlowPanel ui:field="teamSelectBoxContainer" />
      </b:ModalBody>
      <b:ModalFooter>
        <b:Button type="LINK" ui:field="cancelButton">Cancel</b:Button>
        <b:Button type="PRIMARY" text="OK" ui:field="selectTeamButton" />
      </b:ModalFooter>
    </b:Modal>
  </g:FlowPanel>
</ui:UiBinder>
